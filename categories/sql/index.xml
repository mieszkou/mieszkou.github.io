&lt;/span>&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="w"> &lt;/span>&lt;span class="n">WAITFOR&lt;/span>&lt;span class="w"> &lt;/span>&lt;span class="n">DELAY&lt;/span>&lt;span class="w"> &lt;/span>&lt;span class="s1">&amp;#39;00:00:05&amp;#39;&lt;/span>&lt;span class="w">
