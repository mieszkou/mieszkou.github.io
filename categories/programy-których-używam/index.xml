&lt;td class="lntd">
&lt;pre tabindex="0" class="chroma">&lt;code class="language-fallback" data-lang="fallback">&lt;span class="line">&lt;span class="cl">WinSCP.com /script=make.txt
&lt;/span>&lt;/span>&lt;/code>&lt;/pre>&lt;/td>&lt;/tr>&lt;/table>
&lt;/div>
&lt;/div>&lt;p>make.txt:&lt;/p>
&lt;div class="highlight">&lt;div class="chroma">
&lt;table class="lntable">&lt;tr>&lt;td class="lntd">
&lt;pre tabindex="0" class="chroma">&lt;code>&lt;span class="lnt">1
&lt;/span>&lt;span class="lnt">2
&lt;/span>&lt;span class="lnt">3
&lt;/span>&lt;span class="lnt">4
&lt;/span>&lt;span class="lnt">5
&lt;/span>&lt;span class="lnt">6
&lt;/span>&lt;/code>&lt;/pre>&lt;/td>
&lt;td class="lntd">
&lt;pre tabindex="0" class="chroma">&lt;code class="language-fallback" data-lang="fallback">&lt;span class="line">&lt;span class="cl">option batch abort
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">option confirm off
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">open ftp://login:haslo@ftp.host.pl
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">synchronize remote &amp;#34;.folder&amp;#34; &amp;#34;/folder/na/ftp/&amp;#34; -filemask=&amp;#34;|.git;*/input/*;*/output/*&amp;#34;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">close
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">exit
&lt;/span>&lt;/span>&lt;/code>&lt;/pre>&lt;/td>&lt;/tr>&lt;/table>
